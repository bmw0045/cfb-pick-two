doctype html
html
    head
        title 2020 Pick Two Pool
        link(rel='stylesheet', href='https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css', integrity='sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh', crossorigin='anonymous')
        link(rel="stylesheet", type="text/css", href="style.css")

        body
            block nav
                nav.navbar.navbar-expand-lg.navbar-dark.bg-dark
                    a.navbar-brand(href='#') 2020 Pick Two Pool
                    button.navbar-toggler(type='button', data-toggle='collapse', data-target='#navbarSupportedContent', aria-controls='navbarSupportedContent', aria-expanded='false', aria-label='Toggle navigation')
                        span.navbar-toggler-icon

                    .collapse.navbar-collapse(id='navbarSupportedContent')
                        ul.navbar-nav.mr-auto
                            li.nav-item.active
                                a.nav-link(href='/') Home
                            li.nav-item
                                a.nav-link(href='/rankings') Top 25
                            li.nav-item
                                a.nav-link(href='/parking') Strike Count
                            li.nav-item
                                a.nav-link(href='/places') Submit Picks
                        ul.navbar-nav.float-right
                            li.nav-item
                                a.nav-link(href='/register') Register
                            li.nav-item
                                a.nav-link(href='/login') Login
                            li.nav-item.dropdown
                                a.nav-link.dropdown-toggle(id='navbarDropdownMenuLink', data-toggle='dropdown', aria-haspopup='true', aria-expanded='false') Accounts
                                .dropdown-menu(aria-labelledby='navbarDropdownMenuLink')
                                    a.dropdown-item(href='#') Settings
            block content